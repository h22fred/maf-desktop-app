	<parameter name="actor_name" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
  projet,
  ressource,
  mois,
  annee,
  round(sum(allocated),1) as allocated,
  round(sum(consumed),1) as consumed
FROM
(

select
  pe.name as PROJET,
  a.ref_id as RESSOURCE,
  allocations.`month` as mois,
  allocations.`year` as annee,
  allocations.allocated as allocated,
  ts.consumed as consumed
FROM
  portfolio_entry pe

  join (
         select
           pe.id as portfolio_entry_id,
           a.id as actor_id,
           perpaad.`month` + 1 as `month`,
           perpaad.`year` as `year`,
           perpaad.days as allocated
         from
           portfolio_entry pe
           join life_cycle_instance lci on pe.active_life_cycle_instance_id = lci.id
           join life_cycle_instance_planning lcip on lci.id = lcip.life_cycle_instance_id and lcip.is_frozen = 0
           join portfolio_entry_resource_plan perp on lcip.portfolio_entry_resource_plan_id = perp.id
           join portfolio_entry_resource_plan_allocated_actor perpaa on perp.id = perpaa.portfolio_entry_resource_plan_id
           join portfolio_entry_resource_plan_allocated_actor_detail perpaad on perpaa.id = perpaad.portfolio_entry_resource_plan_allocated_actor_id
           join actor a on perpaa.actor_id = a.id
       ) allocations on allocations.portfolio_entry_id = pe.id

  join actor a on allocations.actor_id = a.id

  left join (
              select
                pe.id as portfolio_entry_id,
                a.id as actor_id,
                month(tl.log_date) as `month`,
                year(tl.log_date) as `year`,
                sum(tl.hours) / 8 as consumed
              from timesheet_entry te
                join portfolio_entry pe on te.portfolio_entry_id = pe.id
                join timesheet_report tr on te.timesheet_report_id = tr.id
                join actor a on tr.actor_id = a.id
                join timesheet_log tl on te.id = tl.timesheet_entry_id
              group by pe.id, a.id, `year`, `month`
            ) ts on a.id = ts.actor_id and ts.portfolio_entry_id = pe.id 
            and ts.`month` = allocations.`month` and ts.`year` = allocations.`year`

    left join portfolio_entry_type pet on pe.portfolio_entry_type_id = pet.id
    left join org_unit ou on pe.sponsoring_unit_id = ou.id
where a.id = $P{actor_id}
and pe.id = $P{project_id}


and str_to_date(concat(allocations.year, '-', allocations.month, '-', '01'), '%Y-%m-%d') >= date_sub($P{date_report}, interval 4 month)
and str_to_date(concat(allocations.year, '-', allocations.month, '-', '01'), '%Y-%m-%d') <= date_add($P{date_report}, interval 7 month)

union select $P{project_name}, $P{actor_name}, month(date_sub($P{date_report}, interval 4 month)), year(date_sub($P{date_report}, interval 4 month)), 0, 0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_sub($P{date_report}, interval 3 month)), year(date_sub($P{date_report}, interval 3 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_sub($P{date_report}, interval 2 month)), year(date_sub($P{date_report}, interval 2 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_sub($P{date_report}, interval 1 month)), year(date_sub($P{date_report}, interval 1 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month($P{date_report}), year($P{date_report}), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 1 month)), year(date_add($P{date_report}, interval 1 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 2 month)), year(date_add($P{date_report}, interval 2 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 3 month)), year(date_add($P{date_report}, interval 3 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 4 month)), year(date_add($P{date_report}, interval 4 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 5 month)), year(date_add($P{date_report}, interval 5 month)), 0, 0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 6 month)), year(date_add($P{date_report}, interval 6 month)), 0,  0 from dual 
union select $P{project_name}, $P{actor_name}, month(date_add($P{date_report}, interval 7 month)), year(date_add($P{date_report}, interval 7 month)), 0,  0 from dual 

) base
group by projet, ressource, annee, mois
order by projet, ressource, annee, mois]]>
	</queryString>
