	<queryString>
		<![CDATA[select distinct 
  a.id as ressource_id, a.employee_id as ressource_name
from
  portfolio_entry_resource_plan_allocated_actor perpaa
  left join portfolio_entry_resource_plan perp on perpaa.portfolio_entry_resource_plan_id = perp.id
  left join life_cycle_instance_planning lcip on perp.id = lcip.portfolio_entry_resource_plan_id
  left join life_cycle_instance lci on lcip.life_cycle_instance_id = lci.id
  left join portfolio_entry pe on lci.id = pe.active_life_cycle_instance_id
  left join portfolio_has_portfolio_entry phpe on pe.id = phpe.portfolio_entry_id
  left join portfolio p on phpe.portfolio_id = p.id
  left join actor a on perpaa.actor_id = a.id
where perpaa.deleted = 0
      and perp.deleted = 0
      and lcip.deleted = 0 
      and lcip.is_frozen = 0
      and lci.deleted = 0
      and pe.deleted = 0
      and pe.archived = 0
order by ressource_id]]>
	</queryString>
