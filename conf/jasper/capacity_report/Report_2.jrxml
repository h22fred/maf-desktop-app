	<parameter name="date_report" class="java.util.Date"/>
	<queryString>
		<![CDATA[select distinct
  pe.id as project_id,
  pe.name as nom_projet,
  pe.archived as statut_projet
from
  portfolio_entry_resource_plan_allocated_actor perpaa
  left join portfolio_entry_resource_plan perp on perpaa.portfolio_entry_resource_plan_id = perp.id
  left join life_cycle_instance_planning lcip on perp.id = lcip.portfolio_entry_resource_plan_id
  left join life_cycle_instance lci on lcip.life_cycle_instance_id = lci.id
  left join portfolio_entry pe on lci.id = pe.active_life_cycle_instance_id where perpaa.deleted = 0
      and perp.deleted = 0
      and lcip.deleted = 0 and lcip.is_frozen = 0
      and lci.deleted = 0
      and pe.deleted = 0
      and perpaa.end_date >= date_sub($P{date_report}, interval 4 month)
      and perpaa.start_date <= date_add($P{date_report}, interval 7 month) order by pe.name]]>
	</queryString>
	<field name="project_id" class="java.lang.String"/>
