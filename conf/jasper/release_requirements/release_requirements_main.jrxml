	<queryString language="SQL">
		<![CDATA[SELECT 
	r.name AS name, 
	r.id AS id, 
	r.is_defect AS is_defect, 
	IFNULL(rs_i18n.value, '') AS status,
	IFNULL(rp_i18n.value, '') AS priority,
	IFNULL(rsev_i18n.value, '') AS severity,
	r.story_points AS story_points,
	r.initial_estimation AS initial_estimation,
	r.external_ref_id AS external_ref_id,
	r.is_scoped AS is_scoped,
	pe.id AS pe_id, 
	pe.name AS pe_name
FROM

(SELECT i_r.id AS i_r_id FROM requirement i_r 
	JOIN release_portfolio_entry i_rpe ON i_r.portfolio_entry_id = i_rpe.portfolio_entry_id AND i_rpe.release_id = $P{REPORT_RELEASE_REQUIREMENTS_RELEASE} AND i_rpe.type = 'ALL'
UNION
SELECT i_r.id AS i_r_id FROM requirement i_r 
	JOIN release_portfolio_entry i_rpe ON i_r.portfolio_entry_id = i_rpe.portfolio_entry_id AND i_rpe.release_id = $P{REPORT_RELEASE_REQUIREMENTS_RELEASE} AND i_rpe.type = 'BY_REQUIREMENT'
	WHERE i_r.release_id = $P{REPORT_RELEASE_REQUIREMENTS_RELEASE}
UNION
SELECT i_r.id AS i_r_id FROM requirement i_r 
	JOIN release_portfolio_entry i_rpe ON i_r.portfolio_entry_id = i_rpe.portfolio_entry_id AND i_rpe.release_id = $P{REPORT_RELEASE_REQUIREMENTS_RELEASE} AND i_rpe.type = 'BY_ITERATION'
	JOIN iteration i_i ON i_r.iteration_id = i_i.id
	WHERE i_i.deleted = 0 AND i_i.release_id = $P{REPORT_RELEASE_REQUIREMENTS_RELEASE}) rr

JOIN requirement r ON r.id = rr.i_r_id
JOIN portfolio_entry pe ON pe.id = r.portfolio_entry_id

LEFT OUTER JOIN requirement_status rs ON r.requirement_status_id = rs.id
LEFT OUTER JOIN i18n_messages rs_i18n ON rs.name = rs_i18n.key AND rs_i18n.language = $P{lang}

LEFT OUTER JOIN requirement_priority rp ON r.requirement_priority_id = rp.id
LEFT OUTER JOIN i18n_messages rp_i18n ON rp.name = rp_i18n.key AND rp_i18n.language = $P{lang}

LEFT OUTER JOIN requirement_severity rsev ON r.requirement_severity_id = rsev.id
LEFT OUTER JOIN i18n_messages rsev_i18n ON rsev.name = rsev_i18n.key AND rsev_i18n.language = $P{lang}

WHERE pe.deleted = 0 AND r.deleted = 0]]>
	</queryString>
