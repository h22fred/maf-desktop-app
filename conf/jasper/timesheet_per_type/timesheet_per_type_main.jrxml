	<queryString language="SQL">
		<![CDATA[SELECT
'Initiative' as `type`,
a.last_name,
a.first_name,
ou.name as org_unit,
round(sum(hours)/8,2) as days,
round(sum(hours),2) as hours
FROM
timesheet_log tl
join timesheet_entry te on tl.timesheet_entry_id = te.id and te.deleted = 0 and te.portfolio_entry_id is not null
join timesheet_report tr on te.timesheet_report_id = tr.id and tr.deleted = 0 and tr.`status` = 'APPROVED'
join actor a on tr.actor_id = a.id
join org_unit ou on a.org_unit_id = ou.id
where
tl.deleted = 0
and tl.log_date >= $P{TS_TYPE_START_DATE}
and tl.log_date <= $P{TS_TYPE_END_DATE}
and tl.hours != 0
group by
a.id

union

select
ifnull(ta_name.value,'') as `type`,
a.last_name,
a.first_name,
ou.name as org_unit,
round(sum(hours)/8,2) as days,
round(sum(hours),2) as hours
from
timesheet_log tl
join timesheet_entry te on tl.timesheet_entry_id = te.id and te.deleted = 0
join timesheet_report tr on te.timesheet_report_id = tr.id and tr.deleted = 0 and tr.`status` = 'APPROVED'
join timesheet_activity ta on te.timesheet_activity_id = ta.id
left join i18n_messages ta_name on ta_name.`key` = ta.name and ta_name.`language` = $P{lang}
join actor a on tr.actor_id = a.id
join org_unit ou on a.org_unit_id = ou.id
where
tl.deleted = 0
and tl.log_date >= $P{TS_TYPE_START_DATE}
and tl.log_date <= $P{TS_TYPE_END_DATE}
and tl.hours != 0
group by
ta.id,
a.id

order by
`type`,
last_name,
      first_name]]>
	</queryString>
